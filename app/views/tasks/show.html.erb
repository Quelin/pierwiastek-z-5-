<%= link_to("<< Back to List", project_tasks_path(@task.project), class: "btn btn-default") %>
<br /><br /><br />
    <div class="panel panel-info">
        <div class="panel-heading">Task details</div>
        <div class="panel-body">

  <table class = "table table-striped table-hover">
    <tr>
      <th>ID:</th>
      <td><%= @task.id %></td>
    </tr>
    <tr class="active">
      <th>Name:</th>
      <td><%= @task.name %></td>
    </tr>
    <tr>
      <th>Done?:</th>
      <td><%= @task.done %></td>
    </tr>
    <tr  class="active">
      <th>Description:</th>
      <td><%= @task.description %></td>
    </tr>
    <tr>
      <th>Start at:</th>
      <td><%= @task.start_time %></td>
    </tr>
    <tr  class="active">
      <th>End at:</th>
      <td><%= @task.end_time %></td>
    </tr>
    <tr>
      <th>Created at:</th>
      <td><%= @task.created_at %></td>
    </tr>
    <tr  class="active">
      <th>Updated at:</th>
      <td><%= @task.updated_at %></td>
    </tr>
  </table>
                </div>
              </div>